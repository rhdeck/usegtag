{
  "name": "usegtag",
  "private": false,
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "setup": "./init-template.js",
    "pre-commit": "(git-branch-is --not main || (set -u && echo $ALLOWMAIN)) && yarn test",
    "test": "tsc --noEmit",
    "build": "tsc && yarn pack -f package.tgz",
    "docs:lib": "typedoc --theme markdown --readme none --includeVersion --out docstemp src/index.ts && rm README.md ; mv docstemp/README.md docstemp/_library.md",
    "docs:compile": "mkdir docstemp ; cp TOP.md docstemp/README.md ; concat-md docstemp > README.md && rm -rf docstemp",
    "docs": "rm -rf docstemp && yarn docs:lib  && yarn docs:compile",
    "prepublishOnly": "yarn build && yarn docs && git commit README.md -m \"docs: updating docs\" && git push",
    "watch": "livelink watch",
    "prepare": "install-peers"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhdeck/usegtag.git"
  },
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https:/github.com/rhdeck/usegtag/issues"
  },
  "homepage": "https://github.com/rhdeck/usegtag#README",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@raydeck/livelink": "^1.5.2",
    "@types/gtag.js": "^0.0.4",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.3",
    "@types/validate-npm-package-name": "^3.0.2",
    "concat-md": "^0.3.5",
    "git-branch-is": "^4.0.0",
    "husky": "^4.3.6",
    "install-peers-cli": "^2.2.0",
    "typedoc": "^0.20.28",
    "typedoc-plugin-markdown": "^3.5.0",
    "typescript": "^4.2.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {},
  "liveLink": {
    "ignoreMasks": [
      "package.tgz",
      "lib/**",
      "README.md",
      "docs/**",
      "docstemp/**"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
